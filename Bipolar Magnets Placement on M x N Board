#include <stdio.h>

int main() {
    int M = 4; // Number of rows
    int N = 6; // Number of columns

    for (int i = 0; i < M; i++) {
        for (int j = 0; j < N; j++) {
            if ((i + j) % 2 == 0) {
                printf("[+] ");
            } else {
                printf("[-] ");
            }
        }
        printf("\n");
    }

    return 0;
}
